.inners_swiper{
  width: 710rpx;
  margin: 0 auto;
  border-radius: 10px;
  margin-top: 10px;
}


/* 回收类别 */
.reserve{
  width: 750rpx;
  height: auto;
  padding-bottom: 110rpx;
}
.reserve_kind .title{
  display: flex;
  align-items: center;
}
.reserve_kind .title .lei{
  color: #31C0A8;
  font-size: 28px;
  line-height: 60rpx;
  text-align: center;
}
.reserve .reserve_kind .title .saf{
  font-weight: bold;
  padding: 15rpx;
}
 .reserve_kind .title view{
  width: 40rpx;
  height: 60rpx;
  background: none;
  border-radius: 50%;
  position: relative;
  margin-left: 18rpx;
  margin-right: 12rpx;
  /* font-weight: bold; */
}
.reserve_kind .title view image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.reserve_kind .select{
  width: 720rpx;
  height: auto;
  display: flex;
  justify-content: space-around;
  margin: 10rpx auto;
}
.reserve_kind .select image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
}
.reserve_kind .select .every view{
  color: rgba(16, 16, 16, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  text-align: center;
  /* font-weight: bold; */
}

/* 重量估计 */
.reserve_weight .title{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.reserve_weight .photo{
  display: flex;
  align-items: center;
}
.reserve_weight .photo text{
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
  font-weight: bold;
}
.reserve_weight .title .tit{
  color: rgba(16, 16, 16, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  margin-right: 30rpx;
}
.reserve_weight .photo view{
  width: 10rpx;
  height: 60rpx;
  background: none;
  border-radius: 50%;
  position: relative;
  margin-left: 18rpx;
  margin-right: 0rpx;
}
.reserve_weight .photo .zhong{
  width: 30px;
  height: 30px;
  background: none;
  border-radius: 50%;
  color: #31C0A8;
  font-size: 26px;
  text-align: center;
  line-height: 30px;
  margin-left:17rpx;
  margin-right:8rpx;
}
.reserve_weight .inner{
  display: flex;
  justify-content: space-between;
  width: 634rpx;
  margin: 0 auto;
  flex-wrap: wrap;
}
.reserve_weight .inner view{
  width: 300rpx;
  height: 72rpx;
  background: #ffffff;
  border-radius: 8rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  text-align: center;
  line-height: 72rpx;
  margin: 15rpx 0;
}
.reserve_weight .inner .shows{
  background: #31C0A8;
  color: #ffffff;
}

/* 地址信息 */
.reserve_address .title{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.reserve_address .photo{
  display: flex;
  align-items: center;
}
.reserve_address .tit{
  display: flex;
  align-items: center;
}
.reserve_address .photo text{
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
  font-weight: bold;
}
.reserve_address .photo view{
  width: 60rpx;
  height: 60rpx;
  background: none;
  border-radius: 50%;
  position: relative;
  margin-left: 18rpx;
  margin-right: 8rpx;
}
.reserve_address .photo .xinx{
  font-size: 26px;
  color: #31C0A8;
  line-height: 60rpx;
  text-align: center;
  margin-left: 2px;
}
.reserve_address .photo view image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.reserve_address .tit view{
  width: 40rpx;
  height: 40rpx;
  background: #31C0A8;
  border-radius: 50%;
  position: relative;
  margin-right: 14rpx;
}
.reserve_address .tit view image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.reserve_address .tit text{
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  margin-right: 56rpx;
}

.reserve_address .add_mess{
  width: 630rpx;
  height: auto;
  background: #ffffff;
  margin: 10rpx auto;
  border-radius: 16rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  padding: 20rpx 0;
}
.reserve .reserve_address .add_mess .select{
  color: rgba(171, 168, 172, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.reserve_address .add_mess text{
  line-height: 70rpx;
  margin-right: 80rpx;
}
.reserve_address .add_mess image{
  width: 20rpx;
  height: 20rpx;
}
.reserve_address .add_mess view{
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  line-height: 40rpx;
}
.reserve_address .add_mess .title{
  width: 480rpx;
  padding-bottom: 20rpx;
}
.reserve_address button{
  width: 500rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #31C0A8;
  background-color: #ffffff;
  border: none;
  outline: 0;
}
.btn-hover{
  background: #ffffff;
}


/* 上门时间 */
.reserve_time .title{
  display: flex;
  align-items: center;
}
.reserve_time .title text{
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
  font-weight: bold;
  margin-top: 8rpx;
}
.reserve_time .title view{
  width: 60rpx;
  height: 60rpx;
  background: none;
  border-radius: 50%;
  position: relative;
  margin-left: 18rpx;
  margin-right: 8rpx;
}.reserve_time .title .tis{
  color: #31C0A8;
  font-size: 26px;
  /* text-align: center; */
  margin-left: 7rpx;
  line-height: 30px;
}
.reserve_time .title view image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.reserve_time .inner{
  width: 636rpx;
  height: 200rpx;
  background: #ffffff;
  margin: 10rpx auto;
  border-radius: 15rpx;
}
.reserve_time .inner image{
  width: 24rpx;
  height: 24rpx;
}
.reserve_time .inner .time{
  width: 536rpx;
  height: 96rpx;
  margin: 0 auto;
  
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.reserve_time .inner .time .picker{
  color: rgba(171, 168, 172, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  /* border: none;
  background: #ffffff; */
  /* border-bottom: 2rpx solid #BBBBBB; */
}
.reserve_time .inner .remake{
  width: 536rpx;
  height: 96rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 2rpx solid #BBBBBB;
}
.reserve_time .inner .remake .rights{
  margin-top: 10rpx;
}
.reserve_time .inner .remake view{
  color: rgba(6, 6, 6, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.reserve_time .inner .remake .remake_tie{
  width: 456rpx;
  display: flex;
  justify-content: space-between;
}
.reserve_time .inner .remake .remake_tie input{
  color: #666666;
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}

/* 勾选协议 */
.reserve_check{
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 32rpx 0;
}
.reserve_check .checked{
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #BBBBBB;
  margin-right: 10rpx;
}
.reserve_check .checked image{
  width: 30rpx;
  height: 30rpx;
}
.reserve_check view{
  color: #101010;
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.reserve_check view text{
  color: rgb(45, 126, 180);
}

/* 提交 */
.reserve_sumbit{
  width: 750rpx;
  height: 110rpx;
  position: fixed;
  background: #ffffff;
  left: 0;
  bottom: 0;
  z-index: 999;
}
.reserve_sumbit button{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 654rpx;
  height: 84rpx;
  background: #31C0A8;
  border-radius: 40rpx;
  text-align: center;
  line-height: 84rpx;
  color: rgba(248, 248, 248, 1);
  font-size: 36rpx;
  font-family: PingFangSC-bold;
  outline: 0;
  border: 0;
}


