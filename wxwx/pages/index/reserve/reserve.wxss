  background: #ffffff;
}
.liyg .middle image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  /* background: red; */
}
.liyg .middle view{
  color: rgba(49, 192, 168, 1);
  font-size: 36rpx;
  font-family: 方正准圆-标准;
