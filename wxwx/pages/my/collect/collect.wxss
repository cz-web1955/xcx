/* pages/my/collect/collect.wxss */
.collect{
  width: 750rpx;
  height: auto;
  background: #f5f5f5;
  /* box-sizing: border-box;
  padding: 20rpx; */
}
.collect .title{
  display: flex;
  align-items: center;
  padding-bottom: 10rpx;
  border-bottom: 1px solid #f1f1f1;
}
.collect .whites{
  background: #ffffff;
  box-sizing: border-box;
  padding: 20rpx;
  margin: 8rpx 0;
}
.collect .title .sps{
  color: #101010;
  font-size: 23px;
}
.collect .title .ssr{
  font-size: 20px;
  color: #999999;
}
.collect .title image{
  width: 48rpx;
  height: 48rpx;
}
.collect .title image:nth-child(3){
  width: 24rpx;
  height: 24rpx;
}
.collect .title .asd{
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
  margin: 0 12rpx;
}
.collect .inner{
  display: flex;
  margin: 15rpx 0;
  position: relative;
}
.collect .inner .photo{
  width: 200rpx;
  height: 200rpx;
  margin-right: 26rpx;
}
.collect .inner .photo image{
  width: 200rpx;
  height: 200rpx;
}
.collect .inner .txt{
  display: flex;
  flex-direction: column;
}
.collect .inner .txt view:nth-child(1){
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  line-height: 42rpx;
   text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.collect .inner .txt .price{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 62rpx;
  width: 460rpx;
}
.collect .inner .txt .price .del{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 152rpx;
  height: 50rpx;
  border: 1rpx solid #dedede;
  border-radius: 30rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #101010;
}
.collect .inner .txt .price .num{
  position: absolute;
  left: 230rpx;
  bottom: 0;
}
.collect .inner .txt .price .num .nsb{
  color: #FFB640;
  margin: 0 4rpx;
}
.collect .inner .txt .price .num image{
  width: 32rpx;
  height: 32rpx;
  display: inline-block;
  vertical-align: middle;
}
.collect .inner .txt .price .num text:nth-child(1){
  color: #31C0A8;
  font-size: 28rpx;
}
.collect .inner .txt .price .num text:nth-child(3){
  color: #FFB640;
  font-size: 28rpx;
}
.collect .els view{
  text-align: center;
  line-height: 200rpx;
}
.collect .els text{
  color: #31c0a8;
  text-decoration: underline;
}